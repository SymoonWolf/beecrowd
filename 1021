#include<stdio.h>
int main()
{
    int b;
    double a;
    scanf("%lf", &a);
    b = a*100;
    printf("NOTAS:\n%d nota(s) de R$ 100.00\n%d nota(s) de R$ 50.00\n%d nota(s) de R$ 20.00\n%d nota(s) de R$ 10.00\n%d nota(s) de R$ 5.00\n%d nota(s) de R$ 2.00\nMOEDAS:\n%d moeda(s) de R$ 1.00\n%d moeda(s) de R$ 0.50\n%d moeda(s) de R$ 0.25\n%d moeda(s) de R$ 0.10\n%d moeda(s) de R$ 0.05\n%d moeda(s) de R$ 0.01\n", b/10000, b%10000/5000, b%10000%5000/2000, b%10000%5000%2000/1000, b%10000%5000%2000%1000/500, b%10000%5000%2000%1000%500/200, b%10000%5000%2000%1000%500%200/100, b%10000%5000%2000%1000%500%200%100/50, b%10000%5000%2000%1000%500%200%100%50/25, b%10000%5000%2000%1000%500%200%100%50%25/10, b%10000%5000%2000%1000%500%200%100%50%25%10/5, b%10000%5000%2000%1000%500%200%100%50%25%10%5/1);
    return 0;
}
