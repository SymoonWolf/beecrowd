#include<stdio.h>
#include<math.h>
int main()
{
    float a,b,c,d,e,f;
    scanf("%f%f%f", &a,&b,&c);
    d = b*b - 4*a*c;
    if(d<0 || a == 0)
        printf("Impossivel calcular\n");
    else
    {
        e=(-b + sqrt(d))/(a+a);
        f=(-b - sqrt(d))/(a+a);
        printf("R1 = %.5f\nR2 = %.5f\n", e, f);
    }
        return 0;
}

