#include <stdio.h>
 
int main() {
 
    /**
     * Escreva a sua solução aqui
     * Code your solution here
     * Escriba su solución aquí
     */
     float a, b, c, rec, cir, trap, sq, tri;
    scanf("%f%f%f", &a, &b, &c);
    rec = a*b;
    cir = 3.14159*c*c;
    trap = (a+b)*c*.5;
    sq = b*b;
    tri = 0.5*a*c;
    printf("TRIANGULO: %.3f\nCIRCULO: %.3f\nTRAPEZIO: %.3f\nQUADRADO: %.3f\nRETANGULO: %.3f\n", tri, cir, trap, sq, rec);

    return 0;
}
