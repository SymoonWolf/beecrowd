#include<stdio.h>
int main()
{
     int a;
    scanf("%d", &a);
    printf("%d\n%d nota(s) de R$ 100,00\n%d nota(s) de R$ 50,00\n%d nota(s) de R$ 20,00\n%d nota(s) de R$ 10,00\n%d nota(s) de R$ 5,00\n%d nota(s) de R$ 2,00\n%d nota(s) de R$ 1,00\n", a, a/100, a%100/50, a%100%50/20, a%100%50%20/10, a%100%50%20%10/5, a%100%50%20%10%5/2, a%100%50%20%10%5%2/1);
    return 0;
}
